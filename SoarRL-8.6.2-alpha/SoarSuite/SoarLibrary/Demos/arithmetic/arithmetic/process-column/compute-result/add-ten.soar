sp {compute-result*propose*add-ten
   (state <s> ^name compute-result
              ^current-column.new-digit1 <r>
              ^result <r>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name add-ten)
}

sp {compute-result*apply*add-ten
   (state <s> ^operator <op>
              ^arithmetic.add10-facts <af>
              ^superstate <ss>)
   (<ss> ^digit1 <d>)
   (<af> ^digit1 <d>
         ^digit-10 <d10>)
   (<op> ^name add-ten)
-->
   (<ss> ^digit1 <d> -
                 <d10>)
}


