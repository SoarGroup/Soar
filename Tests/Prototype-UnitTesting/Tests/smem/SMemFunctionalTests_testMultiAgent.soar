smem --set learning on

sp {propose*init
   (state <s> ^superstate nil
             -^name)
-->
   (<s> ^operator.name init)
}

sp {apply*init
   (state <s> ^operator.name init)
-->
   (<s> ^random-objects <ro>
        ^name multiagent)
   (<ro> ^random-1 (random-int)
   	     ^random-2 (random-int)
         ^random-3 (random-int)
         ^random-4 (random-int)
         ^random-5 (random-int)
         ^random-6 (random-int)
         ^random-7 (random-int)
         ^random-8 (random-int)
         ^random-9 (random-int)
         ^random-10 (random-int))
}

sp {propose*store
   (state <s> ^name multiagent
              ^random-objects <ro>)
   (<ro> -^stored true)
-->
   (<s> ^operator.name store)
}

sp {apply*store
   (state <s> ^operator.name store
              ^random-objects <ro>
              ^smem.command <sc>)
   (<ro> ^random-1 <r1>
         ^random-2 <r2>
         ^random-3 <r3>
         ^random-4 <r4>
         ^random-5 <r5>
         ^random-6 <r6>
         ^random-7 <r7>
         ^random-8 <r8>
         ^random-9 <r9>
         ^random-10 <r10>)
-->
   (<sc> ^store <ro>)
   (<ro> ^stored true)
   (<s> ^storage <storage>)
   (<storage> ^random-1 <r1>
        	  ^random-2 <r2>
        	  ^random-3 <r3>
         	  ^random-4 <r4>
       	      ^random-5 <r5>
       	      ^random-6 <r6>
       	      ^random-7 <r7>
       	      ^random-8 <r8>
              ^random-9 <r9>
              ^random-10 <r10>)
}

sp {propose*done
   (state <s> ^name multiagent
              ^random-objects <ro>)
   (<ro> ^stored true)
-->
   (<s> ^operator.name done)
}

sp {apply*done
   (state <s> ^operator.name done
              ^random-objects <ro>
              ^smem.command <sc>)
   (<sc> ^store <store>)
-->
   (<s> ^random-objects <ro> -)
   (<sc> ^store <store> -)
}

sp {propose*query
   (state <s> -^random-objects
               ^name multiagent)
-->
   (<s> ^operator.name query)
}

sp {apply*query
   (state <s> ^operator.name query
              ^smem.command <c>)
-->
   (<c> ^query.random-1 <ro>)
}

sp {success
   (state <s> ^smem.result.success <q>
              ^smem.result.retrieved <r>
              ^storage <storage>)
   (<r> ^random-1 <r1>
        ^random-2 <r2>
        ^random-3 <r3>
        ^random-4 <r4>
        ^random-5 <r5>
        ^random-6 <r6>
        ^random-7 <r7>
        ^random-8 <r8>
        ^random-9 <r9>
        ^random-10 <r10>)
   (<storage> ^random-1 <r1>
              ^random-2 <r2>
              ^random-3 <r3>
              ^random-4 <r4>
              ^random-5 <r5>
              ^random-6 <r6>
              ^random-7 <r7>
              ^random-8 <r8>
              ^random-9 <r9>
              ^random-10 <r10>)
-->
   (exec succeeded)
}
