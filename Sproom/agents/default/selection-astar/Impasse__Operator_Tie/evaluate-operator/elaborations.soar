##### Avoid redoing
sp {evaluate-operator*select
   (state <s> ^name evaluate-operator
              ^quiescence t
              ^superstate.operator.evaluation.visited <v>
              ^operator <op1> +)
   (<op1> ^next-waypoint <v>)
   (<v> ^id <vid>)
-->
   (write (crlf) |Reject moving to | <vid>)
   (<s> ^operator <op1> -)
}
sp {evaluate-operator*select*local
   (state <s> ^name evaluate-operator
              ^quiescence t
              ^visited <v>
              ^operator <op1> +)
   (<op1> ^next-waypoint <v>)
   (<v> ^id <vid>)
-->
   (write (crlf) |Reject moving to | <vid>)
   (<s> ^operator <op1> -)
}



