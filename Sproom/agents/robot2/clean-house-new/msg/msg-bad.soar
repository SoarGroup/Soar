sp {msg*propose*msg-bad
   (state <s> ^name msg
              ^message.first.word <w>)
  -(<s> ^supported <w>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name msg-bad)
}

sp {apply*msg-bad
   (state <s> ^operator <op>
              ^superstate.operator <ss-op>)
   (<op> ^name msg-bad)
-->
   (<ss-op> ^processed true)
}
