sp {go-to*propose*stop*in-range
   (state <s> ^name go-to
              ^yaw-aligned true
              ^destination-in-range yes
              ^top-state.io.input-link <input>)
   (<input> ^self.stopped no)
-->
   (write (crlf) |Go-to block stop.|)
   (<s> ^operator <op> + =, >)
   (<op> ^name stop
         ^actions.stop <stop>)
}

