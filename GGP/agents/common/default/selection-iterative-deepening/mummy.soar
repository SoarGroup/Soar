sp {mummy*elaborate*state*minimum-depth
   (state <s> ^gs.location <l1> <l2>
              ^elaborations <e>)
   (<l1> ^p1 explorer ^p2 <x1> ^p3 <y1>)
   (<l2> ^p1 exit ^p2 <x2> ^p3 <y2>)
-->
   (<e> ^minimum-explorer-exit-distance (+ (abs (- <x1> <x2>)) (abs (- <y1> <y2>))))
}
