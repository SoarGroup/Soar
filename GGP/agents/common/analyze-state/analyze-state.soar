sp {elaborate*ts*goodthings
   (state <s> ^superstate nil)
-->
   (<s> ^good-things <gt>)
}

sp {apply*analyze-state*goodthing
   (state <s> ^name game
              ^operator.name analyze-state
              ^gs <gs>
              ^duplicates <dup>
              ^current-evaluation-depth <ced>
              ^top-state.good-things <gt>)
   (<dup> ^<old-gs> <gs>)
   (<gt> ^<item> <id-gt>)
   (<id-gt> ^p1 <p1-val>)
   (<gs> ^<item> <id-gs>)
   (<id-gs> ^p1 <p1-val>)
  -{(<old-gs> ^<item> <id-oldgs>)
    (<id-oldgs> ^p1 <p1-val>)}
-->
   (<s> ^current-evaluation-depth <ced> -
                                  (+ <ced> 1))
   (write (crlf) |Bump due to appearence of | <item> | | <p1-val>)
}

sp {apply*update-state*copy-gs*deep
  (state <s> ^operator.name update-state
             ^gs <gs>
             ^old-gs <ogs>)
  (<gs> ^<att> <id>)
  (<id> ^p1 <val>
        -^p2)
-->
  (<ogs> ^<att> <new-id>)
  (<new-id> ^p1 <val>)
}

sp {apply*update-state*copy-gs*deep2
  (state <s> ^operator.name update-state
             ^gs <gs>
             ^old-gs <ogs>)
  (<gs> ^<att> <id>)
  (<id> ^p1 <val>
        ^p2 <val2>
        -^p3)
-->
  (<ogs> ^<att> <new-id>)
  (<new-id> ^p1 <val>
            ^p2 <val2>)
}

sp {apply*update-state*copy-gs*deep3
  (state <s> ^operator.name update-state
             ^gs <gs>
             ^old-gs <ogs>)
  (<gs> ^<att> <id>)
  (<id> ^p1 <val>
        ^p2 <val2>
        ^p3 <val3>)
-->
  (<ogs> ^<att> <new-id>)
  (<new-id> ^p1 <val>
            ^p2 <val2>
            ^p3 <val3>)
}


sp {apply*update-state*remove-old-copy
  (state <s> ^operator.name <> update-state <> analyze-state <name>
             ^gs <gs>
             ^old-gs <ogs>)
  (<ogs> ^<att> <id>)
-->
  (<ogs> ^<att> <id> -)
}

  
sp {apply*analyze-state*top*note-difference
   (state <s> ^name game
              ^superstate nil
              ^operator.name analyze-state
              ^gs <gs>
              ^old-gs <old-gs>
              ^facts <f>)
   (<gs> ^<item> <id-gs>)
   (<id-gs> ^p1 <p1-val>
            -^p2)
  -{(<old-gs> ^<item> <id-oldgs>)
    (<id-oldgs> ^p1 <p1-val>)}
  -{(<f> ^bookkeeping-state <item>)}
-->
   (write (crlf) |ADDED | <item> | ^p1 | <p1-val>)
}

sp {apply*analyze-state*top*note-difference2
   (state <s> ^name game
              ^superstate nil
              ^operator.name analyze-state
              ^gs <gs>
              ^old-gs <old-gs>)
   (<gs> ^<item> <id-gs>)
   (<id-gs> ^p1 <p1-val>
            ^p2 <p2-val>
            -^p3)
  -{(<old-gs> ^<item> <id-oldgs>)
    (<id-oldgs> ^p1 <p1-val>
                ^p2 <p2-val>)}
  -{(<f> ^bookkeeping-state <item>)}
-->
   (write (crlf) |ADDED | <item> | ^p1 | <p1-val> | ^p2 | <p2-val>)
}

sp {apply*analyze-state*top*note-difference3
   (state <s> ^name game
              ^superstate nil
              ^operator.name analyze-state
              ^gs <gs>
              ^old-gs <old-gs>)
   (<gs> ^<item> <id-gs>)
   (<id-gs> ^p1 <p1-val>
            ^p2 <p2-val>
            ^p3 <p3-val>)
  -{(<old-gs> ^<item> <id-oldgs>)
    (<id-oldgs> ^p1 <p1-val>
                ^p2 <p2-val>
                ^p3 <p3-val>)}
  -{(<f> ^bookkeeping-state <item>)}
-->
